if (doc['date_order'].date.hourOfDay < 5) { return 0 } else if (doc['date_order'].date.hourOfDay < 10) { return 1 } else if (doc['date_order'].date.hourOfDay < 13) { return 2 } else if (doc['date_order'].date.hourOfDay < 17) { return 3 } else if (doc['date_order'].date.hourOfDay < 20) { return 4 } else { return 5 }
