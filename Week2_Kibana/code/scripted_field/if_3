if (doc['주문시간'].value == 1) { 
  return "월" 
} 
else if (doc['주문시간'].value == 2) { 
  return "화" 
} 
else if (doc['주문시간'].value == 3) { 
  return "수" 
} 
else if (doc['주문시간'].value == 4) { 
  return "목" 
} 
else if (doc['주문시간'].value == 5) { 
  return "금" 
} 
else if (doc['주문시간'].value == 6) {
  return "토" 
}
else {
  return "일"
}
